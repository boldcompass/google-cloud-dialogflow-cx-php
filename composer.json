{
  "name": "boldcompass/google-cloud-dialogflow-cx",
  "description": "Google Cloud Dialogflow CX Client for PHP",
  "license": "Apache-2.0",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Google\\Cloud\\Dialogflow\\Cx\\": "src",
      "GPBMetadata\\Google\\Cloud\\Dialogflow\\Cx\\": "metadata"
    }
  },
  "extra": {
    "component": {
      "id": "cloud-dialogflow",
      "path": "Dialogflow",
      "entry": null,
      "target": "googleapis/google-cloud-php-dialogflow.git"
    }
  },
  "require": {
    "google/gax": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0",
    "google/cloud-core": "^1.39",
    "phpdocumentor/reflection": "^3.0"
  },
  "suggest": {
    "ext-grpc": "Enables use of gRPC, a universal high-performance RPC framework created by Google."
  }
}
